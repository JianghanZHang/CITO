    <parent link="base_link"/>
    <child link="trifinger_base_link"/>
    <!-- Rotation of -90 degrees (in radians) around Z-axis -->
    <origin xyz="0 0 0.283" rpy="0 0 -1.57079632679"/>
  </joint>

